{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/admin/(.*)",
            "dest": "/app.js"
        },
        {
            "src": "/user/(.*)",
            "dest": "/app.js"
        }
    ]
}